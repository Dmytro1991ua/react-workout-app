{
  "name": "react-workout-app",
  "proxy": "http://localhost:8080",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "6.14.13",
    "node": "14.17.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.8",
    "@reduxjs/toolkit": "^1.8.1",
    "@styled-icons/fa-solid": "^10.26.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@types/jest": "^27.0.3",
    "@types/leaflet": "^1.7.6",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-leaflet": "^2.8.2",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.16",
    "@types/swiper": "^5.4.3",
    "axios": "^0.21.1",
    "firebase": "^9.8.4",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.25.3",
    "react-leaflet": "^3.1.0",
    "react-loader-spinner": "^5.1.3",
    "react-modal": "^3.14.4",
    "react-redux": "^8.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.8.0",
    "react-toastify": "^8.1.0",
    "react-tooltip": "^4.2.21",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.2.1",
    "styled-icons": "^10.45.0",
    "swiper": "^6.5.0",
    "uuid": "^8.3.2",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepare": "node -e \"if (process.env.REACT_APP_NODE_ENV !== 'production'){process.exit(1)} \" || husky install",
    "prettier": "prettier --check './src/**/*.{tsx,ts}'",
    "prettier:fix": "prettier --write ./src/",
    "lint": "eslint ./src --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix:errors-only": "npm run lint -- --quiet",
    "lint:staged": "lint-staged --relative",
    "pre-commit-check": "npm run lint:staged",
    "stylelint": "stylelint ./src/**/*.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run prettier:fix",
      "npm run stylelint"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react-modal": "^3.13.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.7.3"
  }
}
